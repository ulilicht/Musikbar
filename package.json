{
  "name": "musikbar",
  "productName": "Musikbar",
  "version": "2.0.1",
  "description": "A Mac Menubar app to control Music Assistant.",
  "author": "Ulrich Lichtenegger",
  "private": true,
  "main": "electron/main.js",
  "homepage": "./",
  "engines": {
    "node": "18"
  },
  "dependencies": {
    "debug": "^4.4.3",
    "electron-store": "8.2.0",
    "menubar": "^9.5.2",
    "new-github-issue-url": "^1.1.0"
  },
  "devDependencies": {
    "@electron-forge/cli": "^7.10.2",
    "@electron-forge/maker-dmg": "^7.10.2",
    "@electron-forge/maker-zip": "^7.10.2",
    "electron": "^39.2.7",
    "react": "^19.2.3",
    "react-dom": "^19.2.3",
    "react-feather": "^2.0.10",
    "react-rangeslider": "^2.2.0",
    "react-scripts": "5.0.1"
  },
  "scripts": {
    "start": "export BROWSER=none && react-scripts start",
    "start-electron": "export ELECTRON_START_URL=http://localhost:3000 && export RAUMBAR_DEVELOPMENT_MODE=true && electron-forge start",
    "build": "react-scripts build",
    "eject": "react-scripts eject",
    "package": "react-scripts build && electron-forge package",
    "make": "react-scripts build && electron-forge make --arch=universal"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "config": {
    "forge": "./forge.config.js"
  }
}
